<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:adnroid="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.practicum.p0972_servicebindserver" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <service android:name="com.practicum.p0972_servicebindserver.MyService"
            tools:ignore="IntentFilterExportedReceiver,MissingClass">
            <intent-filter>
                <action adnroid:name="com.practicum.p0972_servicebindserver.MyService"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </service>
    </application>

</manifest>